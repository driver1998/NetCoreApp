<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	  <AssemblyName>System.Runtime.InteropServices.WindowsRuntime</AssemblyName>
	  <PublicSign>True</PublicSign>
	  <SignAssembly>true</SignAssembly>
	  <Version>4.0.0.0</Version>
	  <AssemblyOriginatorKeyFile>../Snks/MSFT.snk</AssemblyOriginatorKeyFile>
	  <PublicKey></PublicKey>
	  <PublicKeyToken>b03f5f7f11d50a3a</PublicKeyToken>
	  <DelaySign>false</DelaySign>
  </PropertyGroup>

	<PropertyGroup>
		<WindowsSdkPath Condition="'$(WindowsSdkPath)'==''">$([MSBuild]::GetRegistryValueFromView('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Microsoft SDKs\Windows\v10.0', 'InstallationFolder', null, RegistryView.Registry32, RegistryView.Default))</WindowsSdkPath>
		<TargetPlatformVersion>10.0.22621.0</TargetPlatformVersion>
	</PropertyGroup>

	<ItemGroup>
		<ReferencePath Include="$(WindowsSdkPath)\References\$(TargetPlatformVersion)\Windows.Foundation.FoundationContract\**\*.winmd"></ReferencePath>
	</ItemGroup>
</Project>
