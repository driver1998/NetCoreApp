<Project Sdk="Microsoft.NET.Sdk">
	<Import Project="$(MSBuildProjectDirectory)\..\Props\Mile.Xaml.props" />

	<PropertyGroup>
		<DefineConstants>DISABLE_XAML_GENERATED_MAIN;DISABLE_XAML_GENERATED_BINDING_DEBUG_OUTPUT;DISABLE_XAML_GENERATED_BREAK_ON_UNHANDLED_EXCEPTION</DefineConstants>
		<OutputType>WinExe</OutputType>
		<Platforms>x86;x64;ARM64</Platforms>		
		<RootNamespace>NetCoreApp</RootNamespace>
		<RuntimeIdentifiers>win10-x86;win10-x64;win10-arm64</RuntimeIdentifiers>
		<TargetFramework>net6.0-windows10.0.19041.0</TargetFramework>
		<TargetPlatformVersion>10.0.19041.0</TargetPlatformVersion>
		<TargetPlatformMinVersion>10.0.18362.0</TargetPlatformMinVersion>
		<LangVersion>latest</LangVersion>
		<IsNativeLanguage>true</IsNativeLanguage>
		<MileXamlEnableIntelliSenseForDotNet>false</MileXamlEnableIntelliSenseForDotNet>		
	</PropertyGroup>

	<ItemGroup>
      <PackageReference Include="Dongle.WindowsSDK.NET" Version="10.0.19041.4" />
	</ItemGroup>

</Project>
